	<queryString>
		<![CDATA[SELECT 

YEAR(fechasolicitud) AS AÑO, 
CASE WHEN MONTH(fechasolicitud) = 1 THEN "Enero"
WHEN MONTH(fechasolicitud) = 2 THEN "Febrero"
WHEN MONTH(fechasolicitud) = 3 THEN "Marzo"
WHEN MONTH(fechasolicitud) = 4 THEN "Abril"
WHEN MONTH(fechasolicitud) = 5 THEN "Mayo"
WHEN MONTH(fechasolicitud) = 6 THEN "Junio"
WHEN MONTH(fechasolicitud) = 7 THEN "Julio"
WHEN MONTH(fechasolicitud) = 8 THEN "Agosto"
WHEN MONTH(fechasolicitud) = 9 THEN "Septiembre"
WHEN MONTH(fechasolicitud) = 10 THEN "Octubre"
WHEN MONTH(fechasolicitud) = 11 THEN "Noviembre"
WHEN MONTH(fechasolicitud) = 12 THEN "Diciembre"
ELSE "esto no es un mes" END AS MESespañol,
tipotarifa.NombreTarifa,
count(idsolicitud) as Reservas

FROM `solicitud` inner join tipotarifa on solicitud.idtipotarifa = tipotarifa.idtipotarifa

WHERE idtiposolicitud = 2 and anulado = 0

group by año,MESespañol, solicitud.idtipotarifa]]>
	</queryString>
